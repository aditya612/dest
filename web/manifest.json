
{
    "name": "dest",
    "description": "API client",
    "version": "1.0.1",
    "content_security_policy": {
      "extension_pages": "script-src 'self' ; object-src 'self'"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": "/icons/Icon-192.png"
    },
    "chrome_url_overrides": {
      "newtab": "index.html"
    },

    "permissions": ["tabs"],
    "background" :["backgrounds.js"],
    
    "manifest_version": 3
  }
